BEGIN:VCALENDAR
PRODID:-//learn.extension.org//EN
VERSION:2.0
METHOD:PUBLISH
BEGIN:VEVENT
DTSTART:<%= add_to_cal_timestamp(@event.session_start) %>
DTEND:<%= add_to_cal_timestamp(@event.session_end) %>
SUMMARY:<%= "Learn Event: #{@event.title}" %>
DTSTAMP:<%= add_to_cal_timestamp(Time.zone.now) %>
DESCRIPTION:<%= event_url(@event) %>
UID:<%= @event.id %>
END:VEVENT
END:VCALENDAR